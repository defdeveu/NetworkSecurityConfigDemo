<?xml version="1.0" encoding="utf-8"?>
<network-security-config xmlns:android="http://schemas.android.com/apk/res/android">
    <domain-config>
        <domain includeSubdomains="false">secure.singleframesecurity.net</domain>
        <trust-anchors>
            <certificates src="@raw/caselfsigned"/>
        </trust-anchors>
    </domain-config>

    <domain-config>
        <domain includeSubdomains="false">test.singleframesecurity.net</domain>
        <trust-anchors>
            <certificates src="@raw/cacustomca"/>
        </trust-anchors>
    </domain-config>
</network-security-config>